<MeanCloud_Fishing>

	<append xpath="/progression/perks/perk[@name='perkLivingOffTheLand']">
		<effect_group>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" value="1" level="1,3" tags="Fishing Rod"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" value="1" level="2,3" tags="Fine Fishing Rod"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" value="1" level="3,3" tags="fishTrapItem"/>
		</effect_group>
	</append>

	<append xpath="/progression/perks/perk[@name='perkMasterChef']">
		<effect_group>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" value="1" level="1,5" tags="Boiled Fish,Grilled Fish,Boiled Prawns,Grilled Prawns"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" value="1" level="2,5" tags="Fish Soup,Prepared Sushi"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" value="1" level="3,5" tags="Fish Tacos (Fresh),Surf and Turf"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" value="1" level="4,5" tags="Vinegar,Pickled Fish"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" value="1" level="5,5" tags="Fish and Chips"/>
		</effect_group>
	</append>

</MeanCloud_Fishing>
